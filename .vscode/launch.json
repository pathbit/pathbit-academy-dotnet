{
    "version": "0.2.0",
    "configurations":
    [
        {
            "name": ".NET Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build Path.Infobase.WebHost",
            "program": "${workspaceFolder}/src/WebHost/bin/Debug/net10.0/Path.Infobase.WebHost.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/WebHost",
            "console": "internalConsole",
            "justMyCode": true,
            "stopAtEntry": false,
            "launchBrowser": {
                "enabled": false
            },
            "suppressJITOptimizations": true,
            "suppressMultipleSessionWarning": true,
            "expressionEvaluationOptions": {
                "expandMultiLineLiterals": true,
                "expandStringLiterals": true
            },
            "enableStepFiltering": false,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": false
            },
            "checkForDevCert": false,
            "logging": {
                "moduleLoad": false
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
